{
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3.36,
          "bugfixes": true,
          "modules": false
        }
      ],
      ["@babel/preset-typescript", {
        "optimizeConstEnums": true
      }]
    ],
    "plugins": [
      ["@babel/plugin-transform-runtime", {
        "corejs": 3
      }]
    ]
  }